<?xml version='1.0' encoding='UTF-8'?>
<module rename-to='bourseje'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>

    <inherits name="com.gwtplatform.dispatch.rest.DispatchRest"/>
    <inherits name="com.arcbees.gsss.grid.Grid"/>
    <inherits name="com.arcbees.gsss.animation.Animation"/>
    <inherits name='com.gwtplatform.mvp.MvpWithEntryPoint'/>
    <inherits name="com.google.gwt.query.Query"/>
    <inherits name="org.vectomatic.libgwtsvg"/>

    <source path='client'/>
    <source path='shared'/>

    <set-property name="user.agent" value="safari"/>

    <extend-configuration-property name="gin.ginjector.modules" value="com.arcbees.bourseje.client.ClientModule"/>

    <set-configuration-property name="CssResource.obfuscationPrefix" value="empty"/>
    <set-configuration-property name="gwtp.bootstrapper" value="com.arcbees.bourseje.client.BootstrapperImpl"/>

    <set-configuration-property name="CssResource.legacy" value="true"/>
    <set-configuration-property name="CssResource.enableGss" value="true"/>
</module>
