<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:com.arcbees.bourseje.client.application.ui">

    <ui:with field="res" type="com.arcbees.bourseje.client.resources.Resources"/>
    <ui:with field="page" type="com.arcbees.bourseje.client.resources.PageResultResources"/>

    <g:HTMLPanel styleName="{res.styles.main_section}">
        <w:Header/>

        <section class="{res.styles.main_content}">
            <article>
                <div class="{res.styles.wrapper_desktop} {res.styles.clearfix} {res.styles.txtcenter}">
                    <div class="{page.style.text_intro}">
                        <h2>La jeune personnalité
                            <span>d’affaires 2015</span>
                            est</h2>
                    </div>

                    <div class="{res.grid.col} {res.grid.col_6} {res.grid.col_push_3} {page.style.box_candidates}">
                        <g:Image resource="{res.voteAccent}" styleName="{page.style.avatar}"/>
                        <h3>Jason Lemay</h3>
                        <p>Arcbees</p>
                        <div class="{page.style.number}">
                            <p>100<span>voix</span></p>
                        </div>
                    </div>
                </div>
            </article>
        </section>

        <w:Footer/>
    </g:HTMLPanel>
</ui:UiBinder>
