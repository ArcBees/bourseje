<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:com.arcbees.bourseje.client.application">

    <ui:with field="res" type="com.arcbees.bourseje.client.resources.Resources"/>

    <g:HTMLPanel styleName="{res.styles.height}">
        <w:Header/>

        <div class="{res.styles.voteHeader}"/>
        <section class="{res.styles.main} {res.styles.bgBleu}">
            <article class="{res.styles.wrapper}">
                <p>Vous avez choisi.</p>
                <p class="{res.styles.selection}">
                    <g:InlineLabel ui:field="name"/>
                </p>
                <p>Êtes-vous sûr de votre vote ?</p>
                <g:Button ui:field="confirm" styleName="{res.styles.btn} {res.styles.btnConfirm}">Confirmer</g:Button>
            </article>
        </section>

        <w:Footer/>
    </g:HTMLPanel>
</ui:UiBinder>
