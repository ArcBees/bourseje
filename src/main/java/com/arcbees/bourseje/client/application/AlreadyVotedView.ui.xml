<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:com.arcbees.bourseje.client.application">

    <ui:with field="res" type="com.arcbees.bourseje.client.resources.Resources"/>

    <g:HTMLPanel styleName="{res.styles.height}">
        <w:Header/>

        <section>
            <div>
                <article class="{res.styles.wrapper} {res.styles.txtcenter}">
                    <div class="{res.styles.vote}">
                        <g:Image resource="{res.voteNinja}"/>
                    </div>
                    <p>Vous avez déjà voté!<br/>Pas besoin de vous déguiser,<br/>on vous a reconnu!</p>
                    <p>
                        <a href="#" class="{res.styles.jccq}">jccq.qc.ca</a>
                    </p>
                </article>
            </div>
        </section>

        <w:Footer/>
    </g:HTMLPanel>
</ui:UiBinder>
