<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:svg="urn:import:org.vectomatic.dom.svg.ui">

    <ui:with field="res" type="com.arcbees.bourseje.client.resources.Resources"/>

    <g:HTMLPanel tag="footer">
        <div class="{res.styles.wrapper} {res.styles.clearfix}">
            <div class="{res.styles.logoJccq}">
                <a href="http://www.jccq.qc.ca/" target="_blank">
                    <svg:SVGImage resource="{res.logoJccq}"/>
                </a>
            </div>
            <div class="{res.styles.logoArcbees}">
                <a href="http://www.arcbees.com" target="_blank">
                    <svg:SVGImage resource="{res.logoArcbees}"/>
                </a>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>