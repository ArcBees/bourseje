<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:com.arcbees.bourseje.client.application">

    <ui:with field="res" type="com.arcbees.bourseje.client.resources.Resources"/>

    <g:HTMLPanel>
        <w:Header/>

        <section>
            <div>
                <article class="wrapper txtcenter">
                    <div class="vote">
                        <g:Image resource="{res.vote}"/>
                    </div>
                    <p>La plateforme de vote de la Jeune Chambre<br/>de Commerce de Qu√©bec
                    </p>
                    <g:Button ui:field="cliquez" styleName="{res.styles.btn} {res.styles.btnSlider}">Cliquez pour passer au vote</g:Button>
                </article>
            </div>
        </section>

        <w:Footer/>
    </g:HTMLPanel>
</ui:UiBinder>
