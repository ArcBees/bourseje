<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:w="urn:import:com.arcbees.bourseje.client.application.ui">

    <ui:with field="res" type="com.arcbees.bourseje.client.resources.Resources"/>

    <g:HTMLPanel styleName="{res.styles.mainSection}">
        <w:Header/>

        <section class="{res.grid.row} {res.styles.wrapper} {res.styles.txtcenter}">
            <article class="{res.grid.col} {res.grid.col_full}">
                <div class="{res.styles.vote}">
                    <g:Image resource="{res.vote}"/>
                </div>
                <p>La plateforme de vote de la Jeune Chambre de Commerce de Qu√©bec</p>
                <g:Button ui:field="cliquez" styleName="{res.styles.btn} {res.styles.btnVotez}">Votez</g:Button>
            </article>
        </section>

        <w:Footer/>
    </g:HTMLPanel>
</ui:UiBinder>
