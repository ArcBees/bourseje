<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:com.arcbees.bourseje.client.application.ui">

    <ui:with field="res" type="com.arcbees.bourseje.client.resources.Resources"/>

    <g:HTMLPanel styleName="{res.styles.mainSection}">
        <w:Header/>

        <section>
            <div>
                <article class="{res.styles.wrapper} {res.styles.txtcenter}">
                    <div class="{res.styles.vote}">
                        <g:Image resource="{res.voteBatman}"/>
                    </div>
                    <p>Nous sommes désolés,<br/>aucun vote n'est en cours.<br/>Mais rassurez-vous, Batman veille.
                    </p>
                    <p>
                        <a href="http://www.jccq.qc.ca" class="{res.styles.jccq}" target="_blank">jccq.qc.ca</a>
                    </p>
                </article>
            </div>
        </section>

        <w:Footer/>
    </g:HTMLPanel>
</ui:UiBinder>
