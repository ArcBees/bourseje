.text_intro h2,
.text_intro p {
    font-size: 2.5rem;
    line-height: 1.2;
}

.text_intro span {
    display: block;
}

.text_intro span:last-of-type {
    font-family: F_PRIMARY;
}

.box_candidates {
    background-color: C_SECONDARY;

    padding: 25px 10px 0;
}

.box_candidates h3 {
    font-size: 3.5rem;
    font-family: F_SECONDARY;
    line-height: 1;

    margin: .75em 0 .25em;
}

.box_candidates p {
    color: C_TEXT_MEDIUM;
    font-size: 2rem;
    text-transform: uppercase;
}

.avatar {
    width: 200px;
    height: 200px;

    vertical-align: middle;

    @mixin rounded(100px);
}

.number {
    background-color: #000;

    margin: 0 -10px -50px -10px;
    padding: 20px 0 10px;
}

.number p {
    color: C_SECONDARY;
    font-family: F_SECONDARY;
    line-height: 1;
    font-size: 5.5rem;

    margin-bottom: 0;
}

.number p span {
    font-size: 2.25rem;

    display: block;
}

.artifice {
    background: url("data:image/svg+xml;base64,          ") no-repeat;
    -webkit-background-size: cover;
    background-size: cover;

    display: block;
    height: 153px;
    width: 160px;
    margin-left: -423px;

    position: absolute;
    bottom: 400px;
    left: 50%;
}

.artifice_1 {
    height: 100px;
    width: 104px;
    margin-left: -375px;

    bottom: 123px;

    @mixin animation_delay(1s);
}

.artifice_2 {
    height: 20px;
    width: 20px;
    margin-right: -430px;

    right: 50%;
    bottom: 650px;
    left: auto;

    @mixin animation_delay(1.25s);
}

.artifice_3 {
    height: 45px;
    width: 45px;
    margin-left: -295px;

    bottom: 75px;

    @mixin animation_delay(1.75s);
}

.artifice_4 {
    margin-right: -400px;
    height: 100px;
    width: 104px;

    right: 50%;
    bottom: 550px;
    left: auto;

    @mixin animation_delay(1.5s);
}

.artifice_5 {
    height: 200px;
    width: 209px;
    margin-left: -500px;

    bottom: 550px;

    @mixin animation_delay(2.5s);
}

.artifice_6 {
    height: 130px;
    width: 136px;
    margin-right: -450px;

    right: 50%;
    bottom: 150px;
    left: auto;

    @mixin animation_delay(2.5s);
